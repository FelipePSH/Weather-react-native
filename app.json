{
  "expo": {
    "name": "AdvisorAPI",
    "slug": "AdvisorAPI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/45px/2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/weatherIcon/45px/2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/weatherIcon/45px/2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "INTERNET"]
    },
    "web": {
      "favicon": "./assets/weatherIcon/45px/2.png"
    }
  }
}
